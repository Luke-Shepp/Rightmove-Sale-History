{
    "name": "Rightmove Sale History",
    "description": "Display property sale history on rightmove search pages",
    "version": "0.0.6",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "css": ["inject.css"],
            "js": ["inject.js"],
            "matches": ["https://www.rightmove.co.uk/*"]
        }
    ]
  }